<!DOCTYPE html>
<html>
  <head>
    <link rel="stylesheet" href="doc.css" type="text/css" media="screen" title="no title" charset="utf-8">
    <title>{{module.name}}</title>
    <script type="text/x-mathjax-config">
    MathJax.Hub.Config({
      tex2jax: {inlineMath: [['$','$'], ['\\(','\\)']]}
    });
    </script>
    <script type="text/javascript" async
      src="https://cdn.mathjax.org/mathjax/latest/MathJax.js?config=TeX-AMS-MML_HTMLorMML">
    </script>
  </head>
  <body>
    <div id="header">
    <h1>Documentation for module <code>{{module.name}}</code>
      {{#if module.version}}<span class="version">(v{{module.version}})</span>{{/if}}
      </h1>
    </div>
    <div id="sidebar">
      <div>
        <h3>Module Index</h3>
      <ul>
        {{#each allModules}}
        <a href="{{module.name}}.html"><li class="depth{{module.depth}}"><code>{{module.name}}</code></li></a>
        {{/each}}
      </ul>
      </div>
      <div>
        <h3>Contents</h3>
      <ul>
        {{#each sortedContents}}{{#if fullName}}
        <a href="#{{makeLink fullName}}"><li><code>{{makeShort fullName}}</code></li></a>
        {{/if}}{{/each}}
      </ul>
      </div>
    </div>
    <div id="main">
      <div class="moduleInfo">
        <h2>Module <code>{{module.name}}</code></h2>
        <p>Author(s): {{{authorLinks module.author}}}<p>
        {{{module.description}}}
      </div>
      {{#each sortedContents}}
      <div>
        {{#if fullName}}<h4 id="{{makeLink fullName}}"><code>{{fullName}}</code></h4>{{/if}}
        {{{description}}}
      </div>
      {{/each}}
    </div>
  </body>
</html>
